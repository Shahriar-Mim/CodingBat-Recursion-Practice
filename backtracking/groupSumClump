public boolean groupSumClump(int start, int[] nums, int target) {
  if(target < 0 ){
    return false; 
  }
  if(start >= nums.length){
    return target == 0; 
  }
  int repeatIndex = adjacentSum(start,nums); 
  int sum = nums[start] * (repeatIndex - start + 1); 
  
   return groupSumClump(repeatIndex+1,nums,target-sum) || 
   groupSumClump(repeatIndex+1,nums,target);
  
}

public int adjacentSum(int start, int[] nums){
  if(start+1 >= nums.length)
    return start; 
  if(nums[start] == nums[start+1])
    return adjacentSum(start+1,nums);
  else 
    return start; 
}
